<body>
    <h2 class="heading">There are total <%= data.length %> users
</h2>
<h5>Admins: (rishav@gmail.com , mangal.radhika5@gmail.com , ashwathpadmanaban39@gmail.com)
</h5>
<table class="table table-striped" id="table">
    <thead>
      <tr>
        <th>Firstname</th>
        <th>Lastname</th>
        <th>Email</th>
      </tr>
    </thead>
    <tbody>
      <% for(var i=0;i<data.length;i++)
      {
          %>

      <tr>
        <td><%= data[i].first %></td>
        <td><%= data[i].last %></td>
        <td><%= data[i].email %></td>
      </tr>
      <%
    }
    %>

    </tbody>
  </table>

